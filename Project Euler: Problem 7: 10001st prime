function nthPrime(n) {
  // Good luck!
  //base case 
  let vecOfPrimes = [2]
  //let potential_prime = 3;

  
function isPrime(arr, number){

  var result = arr.every(function (e) {
    return number % e 
});

if (result == true){
  arr.push(number)
}


}

for(let i = 3; vecOfPrimes.length < 10001; i++){
isPrime(vecOfPrimes, i)

}


return vecOfPrimes

}

console.log(nthPrime(10001));

